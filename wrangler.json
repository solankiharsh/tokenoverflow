{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "tokenoverflow",
  "main": "./workers/app.ts",
  "compatibility_date": "2025-10-08",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "vars": {
    "VALUE_FROM_CLOUDFLARE": "Hello from Cloudflare",
    "VITE_CLERK_PUBLISHABLE_KEY": "",
    "CLERK_PUBLISHABLE_KEY": ""
  },
  "kv_namespaces": [
    {
      "binding": "SUBSCRIBERS",
      "id": "903f4d42bfc54dd88b4e9911535ae9ed"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "tokenoverflow-blog",
      "database_id": "d2144ac7-0842-4461-8ed6-3a1222339f3a"
    }
  ]
}
